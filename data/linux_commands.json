linux_commands='[{"Topic" : "Create bootable USB using \'dd\' command.", "Command" : " sudo dd bs=4M if=path/to/input.iso of=/dev/sd<?> conv=fdatasync  status=progress", "Comment" : "Replace input file path and drive letter(sda/b/c). Use \'lsblk\' if confused.<br> Source - https://askubuntu.com/a/377561"},{"Topic" : "Install .deb file in Linux.", "Command" : " sudo dpkg -i /path/a.deb ", "Comment" : "Use \'sudo apt install -f\' for any missing dependencies.<br> Source - https://unix.stackexchange.com/a/159114"},{"Topic" : "Display the first few words of a file in Linux.", "Command" : " head -c 100 filename ", "Comment" : "Prints first 100 bytes of the file.<br> Source - https://unix.stackexchange.com/a/167816"},{"Topic" : "Display last few words of a file in Linux.", "Command" : " tail -c 100 filename ", "Comment" : "Prints last 100 bytes of the file.<br> Source - https://unix.stackexchange.com/a/167816"},{"Topic" : "Append words to the end of the file in Linux.", "Command" : " echo \'input\' >> filename ", "Comment" : "Attachs the input to the file.<br> Source - https://www.cyberciti.biz/faq/linux-append-text-to-end-of-file/"},{"Topic" : "Insert text into the begining of a file in Linux.", "Command" : " sed -i \'1s/^/abcde /\' filename ", "Comment" : "Attachs the input to the file beginning.<br> Source - https://stackoverflow.com/a/9533736/6903724"}]'





